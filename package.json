{
  "name": "sampleAPI",
  "version": "1.0.0",
  "description": "My Moleculer-based microservices project",
  "scripts": {
    "dev": "moleculer-runner --repl --hot services",
    "start": "moleculer-runner --repl --hot services",
    "cli": "moleculer connect Redis",
    "ci": "jest --watch",
    "test": "jest --coverage",
    "lint": "eslint services",
    "dc:up": "docker-compose up --build -d",
    "dc:logs": "docker-compose logs -f",
    "dc:down": "docker-compose down"
  },
  "keywords": [
    "microservices",
    "moleculer"
  ],
  "author": "",
  "devDependencies": {
    "eslint": "^6.8.0",
    "jest": "^25.1.0",
    "jest-cli": "^25.1.0",
    "moleculer-repl": "^0.6.2"
  },
  "dependencies": {
    "@sendgrid/mail": "^7.2.2",
    "bcrypt": "^5.0.0",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "crypto": "^1.0.1",
    "dotenv": "^8.2.0",
    "helmet": "^4.0.0",
    "ioredis": "^4.14.1",
    "jsonwebtoken": "^8.5.1",
    "moleculer": "^0.14.0",
    "moleculer-db": "^0.8.4",
    "moleculer-db-adapter-mongo": "^0.4.7",
    "moleculer-db-adapter-sequelize": "^0.2.8",
    "moleculer-web": "^0.9.0",
    "oauth2-server": "^3.1.1",
    "path": "^0.12.7",
    "pg": "^8.3.0",
    "pg-hstore": "^2.3.3",
    "qs": "^6.9.4",
    "sequelize": "^5.21.13",
    "uuid": "^3.3.3",
    "uuid-random": "^1.3.0",
    "uuidv4": "^6.2.1"
  },
  "engines": {
    "node": ">= 10.x.x"
  },
  "jest": {
    "coverageDirectory": "../coverage",
    "testEnvironment": "node",
    "rootDir": "./services",
    "roots": [
      "../test"
    ]
  }
}
